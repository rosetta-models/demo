namespace demo.test.reg
version "${project.version}"

func Max: <"Returns the greater number of two supplied numbers.">
    inputs:
        a number (1..1)
        b number (1..1)
    output:
        result number (1..1)
    set result: if a > b then a else b

func MaxRegAttribute: <"Returns the greater number of two supplied numbers.">
    inputs:
        a TestRegExtraAttribute (1..1)
        b TestRegExtraAttribute (1..1)
    output:
        result number (1..1)
    set result: if a -> serialNum > b -> serialNum then a -> serialNum else b -> serialNum

func SumNumbers: <"Returns the sum of the given list of numbers.">
    inputs:
        numbers number (0..*)
    output:
        total number (1..1)

    set total: numbers sum
