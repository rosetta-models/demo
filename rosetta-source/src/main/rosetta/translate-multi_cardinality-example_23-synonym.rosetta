 *             <xs:all>
 *                 <xs:element name="engineType">
 *                     <xs:complexType>
 *                         <xs:sequence>
 *                             <xs:element name="fuelDetail" maxOccurs="unbounded">
 *                                 <xs:complexType>
 *                                     <xs:sequence>
 *                                         <xs:element name="combustible" type="xs:string"/>
 *                                     </xs:sequence>
 *                                 </xs:complexType>
 *                             </xs:element>
 *                             <xs:element name="capacityDetail" maxOccurs="unbounded">
 *                                 <xs:complexType>
 *                                     <xs:sequence>
 *                                         <xs:element name="capacity" type="xs:string"/>
 *                                     </xs:sequence>
 *                                 </xs:complexType>
 *                             </xs:element>
 *                         </xs:sequence>
 *                     </xs:complexType>
 *                 </xs:element>
 *             </xs:all>
 *         </xs:complexType>
 *     </xs:element>
 * </xs:schema>
 */
