 *                                     <xs:all>
 *                                         <xs:element name="metric">
 *                                             <xs:complexType>
 *                                                 <xs:all>
 *                                                     <xs:element name="emissions" type="xs:string" minOccurs="0"/>
 *                                                     <xs:element name="combustible" type="xs:string" minOccurs="0"/>
 *                                                     <xs:element name="capacityUnit" type="xs:string" minOccurs="0"/>
 *                                                 </xs:all>
 *                                             </xs:complexType>
 *                                         </xs:element>
 *                                     </xs:all>
