enum FuelEnum:
	PETROL
	    [synonym CONDITIONAL_SET_EXAMPLE_24 value "Petrol"]
	DIESEL
	    [synonym CONDITIONAL_SET_EXAMPLE_24 value "Diesel"]

enum AlternativeFuelEnum:
	Electricity
	    [synonym CONDITIONAL_SET_EXAMPLE_24 value "Electricity"]

